<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:ShareX.Editor.Views;assembly=ShareX.Editor"
        xmlns:vm="clr-namespace:ShareX.Editor.ViewModels;assembly=ShareX.Editor"
        mc:Ignorable="d" d:DesignWidth="1200" d:DesignHeight="800"
        x:Class="ShareX.Editor.Loader.MainWindow"
        x:DataType="vm:MainViewModel"
        Title="{Binding WindowTitle}"
        WindowState="Maximized"
        WindowStartupLocation="CenterScreen">
  <views:EditorView Name="EditorView" />
</Window>
